{
  "name": "@loaders.gl/schema-utils",
  "version": "4.4.0-alpha.1",
  "description": "Schema-related utilities for loaders.gl...",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/loaders.gl"
  },
  "keywords": [
    "webgl",
    "loader",
    "3d",
    "mesh",
    "point cloud",
    "PLY"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "pre-build": "npm run build-bundle && npm run build-bundle-dev",
    "build-bundle": "ocular-bundle ./bundle.ts --output=dist/dist.min.js",
    "build-bundle-dev": "ocular-bundle ./bundle.ts --env=dev --output=dist/dist.dev.js"
  },
  "dependencies": {
    "@loaders.gl/schema": "4.4.0-alpha.1",
    "@types/geojson": "^7946.0.7",
    "apache-arrow": ">= 15.0.0"
  },
  "peerDependencies": {
    "@loaders.gl/core": "4.4.0-alpha.0"
  },
  "gitHead": "3213679d79e6ff2814d48fd3337acfa446c74099"
}
