{
  "name": "@loaders.gl/3d-tiles",
  "version": "4.0.0-beta.6",
  "description": "3D Tiles, an open standard for streaming massive heterogeneous 3D geospatial datasets.",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/loaders.gl"
  },
  "keywords": [
    "webgl",
    "loader",
    "3d tile",
    "tile",
    "mesh",
    "gltf",
    "point cloud",
    "pointcloud"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "pre-build": "npm run build-bundle && npm run build-bundle -- --env=dev",
    "build-bundle": "ocular-bundle ./src/index.ts"
  },
  "dependencies": {
    "@loaders.gl/draco": "4.0.0-beta.6",
    "@loaders.gl/gltf": "4.0.0-beta.6",
    "@loaders.gl/loader-utils": "4.0.0-beta.6",
    "@loaders.gl/math": "4.0.0-beta.6",
    "@loaders.gl/tiles": "4.0.0-beta.6",
    "@loaders.gl/zip": "4.0.0-beta.6",
    "@math.gl/core": "^4.0.0",
    "@math.gl/geospatial": "^4.0.0",
    "@probe.gl/log": "^4.0.4",
    "long": "^5.2.1"
  },
  "peerDependencies": {
    "@loaders.gl/core": "4.0.0-beta.3"
  },
  "gitHead": "c95a4ff72512668a93d9041ce8636bac09333fd5"
}
