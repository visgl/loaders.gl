{
  "name": "@loaders.gl/flatgeobuf",
  "description": "Loader for FlatGeobuf",
  "version": "4.2.0-alpha.4",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/loaders.gl"
  },
  "keywords": [
    "geometry",
    "loader",
    "parser",
    "FGB",
    "flatgeobuf",
    "cloud optimized",
    "geospatial",
    "table"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "pre-build": "npm run build-worker && npm run build-worker --env.dev && npm run build-bundle && npm run build-bundle-dev",
    "build-bundle": "ocular-bundle ./bundle.ts --output=dist/dist.min.js",
    "build-bundle-dev": "ocular-bundle ./bundle.ts --env=dev --output=dist/dist.dev.js",
    "build-worker": "esbuild src/workers/flatgeobuf-worker.ts --bundle --outfile=dist/flatgeobuf-worker.js --define:__VERSION__=\\\"$npm_package_version\\\""
  },
  "dependencies": {
    "@loaders.gl/gis": "4.2.0-alpha.4",
    "@loaders.gl/loader-utils": "4.2.0-alpha.4",
    "@math.gl/proj4": "^4.0.0",
    "flatgeobuf": "3.27.2"
  },
  "devDependencies": {
    "stream-buffers": "3.0.2"
  },
  "peerDependencies": {
    "@loaders.gl/core": "^4.0.0"
  },
  "gitHead": "c95a4ff72512668a93d9041ce8636bac09333fd5"
}
