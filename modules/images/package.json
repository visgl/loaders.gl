{
  "name": "@loaders.gl/images",
  "version": "4.3.0-alpha.3",
  "description": "Framework-independent loaders and writers for images (PNG, JPG, ...)",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/loaders.gl"
  },
  "keywords": [
    "webgl",
    "loader",
    "3d",
    "mesh",
    "point cloud",
    "PLY"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "pre-build": "npm run build-bundle && npm run build-bundle-dev",
    "build-bundle": "ocular-bundle ./bundle.ts --output=dist/dist.min.js",
    "build-bundle-dev": "ocular-bundle ./bundle.ts --env=dev --output=dist/dist.dev.js"
  },
  "dependencies": {
    "@loaders.gl/loader-utils": "4.3.0-alpha.3"
  },
  "devDependencies": {
    "@types/get-pixels": "^3.3.2"
  },
  "peerDependencies": {
    "@loaders.gl/core": "^4.0.0"
  },
  "gitHead": "3213679d79e6ff2814d48fd3337acfa446c74099"
}
