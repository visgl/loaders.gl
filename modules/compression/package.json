{
  "name": "@loaders.gl/compression",
  "version": "3.0.9",
  "description": "Decompression and compression plugins for loaders.gl",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/loaders.gl"
  },
  "keywords": [
    "webgl",
    "loader",
    "3d",
    "mesh",
    "point cloud"
  ],
  "types": "src/index.ts",
  "main": "dist/es5/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "browser": {
    "lzo": false,
    "zlib": false,
    "util": false
  },
  "scripts": {
    "pre-build": "npm run build-bundle && npm run build-worker",
    "build-bundle": "webpack --display=minimal --config ../../scripts/webpack/bundle.js",
    "build-worker": "webpack --entry ./src/workers/worker.ts --output ./dist/compression-worker.js --env.dev --config ../../scripts/webpack/worker.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "@loaders.gl/loader-utils": "3.0.9",
    "@loaders.gl/worker-utils": "3.0.9",
    "@types/brotli": "^1.3.0",
    "@types/pako": "^1.0.1",
    "lzo-wasm": "^0.0.4",
    "pako": "1.0.11",
    "snappyjs": "^0.6.1"
  },
  "optionalDependencies": {
    "brotli": "^1.3.2",
    "lz4js": "^0.2.0",
    "lzo": "^0.4.11",
    "zstd-codec": "^0.1"
  },
  "devDependencies": {
    "brotli": "^1.3.2",
    "lz4js": "^0.2.0",
    "lzo": "^0.4.11",
    "zstd-codec": "^0.1"
  }
}
