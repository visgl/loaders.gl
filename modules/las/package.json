{
  "name": "@loaders.gl/las",
  "version": "4.0.0-alpha.2",
  "description": "Framework-independent loader for the LAS and LAZ formats",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/loaders.gl"
  },
  "keywords": [
    "webgl",
    "loader",
    "3d",
    "mesh",
    "point cloud",
    "LAS",
    "LAZ"
  ],
  "types": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "sideEffects": false,
  "browser": {
    "fs": false,
    "path": false,
    "ws": false,
    "crypto": false
  },
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "pre-build": "npm run build-bundle && npm run build-worker",
    "build-bundle": "esbuild src/bundle.ts --bundle --outfile=dist/bundle.js",
    "build-worker": "esbuild src/workers/las-worker.js --bundle --outfile=dist/las-worker.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "@loaders.gl/loader-utils": "4.0.0-alpha.2",
    "@loaders.gl/schema": "4.0.0-alpha.2",
    "apache-arrow": "^4.0.0"
  }
}
