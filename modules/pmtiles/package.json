{
  "name": "@loaders.gl/pmtiles",
  "version": "4.0.3",
  "description": "Framework-independent loader for the pmtiles format",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/loaders.gl"
  },
  "keywords": [
    "loader",
    "tile",
    "mvt",
    "image tile",
    "tilejson",
    "range request",
    "cloud native",
    "PM Tiles",
    "pmtiles",
    "webgl",
    "webgpu"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "pre-build": "npm run build-bundle && npm run build-bundle -- --env=dev",
    "build-bundle": "ocular-bundle ./src/index.ts"
  },
  "dependencies": {
    "@loaders.gl/images": "4.0.3",
    "@loaders.gl/loader-utils": "4.0.3",
    "@loaders.gl/mvt": "4.0.3",
    "@loaders.gl/schema": "4.0.3",
    "pmtiles": "^2.7.2"
  },
  "gitHead": "c95a4ff72512668a93d9041ce8636bac09333fd5"
}
