{
  "name": "@loaders.gl/pmtiles",
  "version": "4.3.0-alpha.1",
  "description": "Framework-independent loader for the pmtiles format",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/loaders.gl"
  },
  "keywords": [
    "loader",
    "tile",
    "mvt",
    "image tile",
    "tilejson",
    "range request",
    "cloud native",
    "PM Tiles",
    "pmtiles",
    "webgl",
    "webgpu"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "pre-build": "npm run build-bundle && npm run build-bundle-dev",
    "build-bundle": "ocular-bundle ./bundle.ts --output=dist/dist.min.js",
    "build-bundle-dev": "ocular-bundle ./bundle.ts --env=dev --output=dist/dist.dev.js"
  },
  "dependencies": {
    "@loaders.gl/images": "4.3.0-alpha.1",
    "@loaders.gl/loader-utils": "4.3.0-alpha.1",
    "@loaders.gl/mvt": "4.3.0-alpha.1",
    "@loaders.gl/schema": "4.3.0-alpha.1",
    "pmtiles": "^3.0.4"
  },
  "peerDependencies": {
    "@loaders.gl/core": "^4.0.0"
  },
  "gitHead": "c95a4ff72512668a93d9041ce8636bac09333fd5"
}
