{
  "name": "@loaders.gl/parquet",
  "version": "4.0.0-alpha.24",
  "description": "Framework-independent loader for Apache Parquet files",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/loaders.gl"
  },
  "keywords": [
    "webgl",
    "loader",
    "parser",
    "table",
    "Parquet",
    "Apache Parquet"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/es5/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "pre-build": "npm run build-worker && npm run build-bundle",
    "build-bundle": "esbuild src/bundle.ts --outfile=dist/dist.min.js --bundle --minify --sourcemap --external:{util,fs,path,crypto}",
    "build-worker": "esbuild src/workers/parquet-worker.ts --outfile=dist/parquet-worker.js --bundle --minify --sourcemap --external:{util,fs,path,crypto} --define:__VERSION__=\\\"$npm_package_version\\\""
  },
  "browser": {
    "child_process": false,
    "net": false,
    "tls": false,
    "stream": false,
    "fs": false,
    "./src/buffer-polyfill/buffer-polyfill.node.ts": "./src/browser-polyfill/browser-polyfill.browser.ts",
    "./dist/es5/buffer-polyfill/buffer-polyfill.node.js": "./dist/es5/browser-polyfill/browser-polyfill.browser.js",
    "./dist/esm/buffer-polyfill/buffer-polyfill.node.js": "./dist/esm/browser-polyfill/browser-polyfill.browser.js",
    "./src/lib/wasm/load-wasm/load-wasm-node.ts": "./src/lib/wasm/load-wasm/load-wasm-browser.ts",
    "./dist/es5/lib/wasm/load-wasm/load-wasm-node.js": "./dist/es5/lib/wasm/load-wasm/load-wasm-browser.js",
    "./dist/esm/lib/wasm/load-wasm/load-wasm-node.js": "./dist/esm/lib/wasm/load-wasm/load-wasm-browser.js"
  },
  "comments": [
    "base64-js and ieee754 are used by buffer polyfill"
  ],
  "dependencies": {
    "@loaders.gl/bson": "4.0.0-alpha.24",
    "@loaders.gl/compression": "4.0.0-alpha.24",
    "@loaders.gl/loader-utils": "4.0.0-alpha.24",
    "@loaders.gl/schema": "4.0.0-alpha.24",
    "async-mutex": "^0.2.2",
    "brotli": "^1.3.2",
    "base64-js": "^1.3.1",
    "ieee754": "^1.2.1",
    "int53": "^0.2.4",
    "lz4js": "^0.2.0",
    "node-int64": "^0.4.0",
    "object-stream": "0.0.1",
    "parquet-wasm": "^0.3.1",
    "snappyjs": "^0.6.0",
    "thrift": "^0.14.2",
    "varint": "^5.0.0",
    "zstd-codec": "^0.1"
  },
  "peerDependencies": {
    "apache-arrow": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^10.14.15",
    "@types/node-int64": "^0.4.29",
    "@types/thrift": "^0.10.8",
    "@types/varint": "^5.0.0",
    "apache-arrow": "^13.0.0"
  },
  "gitHead": "c95a4ff72512668a93d9041ce8636bac09333fd5"
}
