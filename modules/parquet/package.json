{
  "name": "@loaders.gl/parquet",
  "version": "3.2.0-alpha.1",
  "description": "Framework-independent loader for Apache Parquet files",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/loaders.gl"
  },
  "keywords": [
    "webgl",
    "loader",
    "parser",
    "table",
    "Parquet",
    "Apache Parquet"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/es5/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "pre-build": "npm run build-worker && npm run build-bundle",
    "build-bundle": "esbuild src/bundle.ts --outfile=dist/dist.min.js --bundle --minify --sourcemap --external:{util,fs,path,crypto}",
    "build-worker": "esbuild src/workers/parquet-worker.ts --outfile=dist/parquet-worker.js --bundle --minify --sourcemap --external:{util,fs,path,crypto} --define:__VERSION__=\\\"$npm_package_version\\\""
  },
  "browser": {
    "child_process": false,
    "net": false,
    "tls": false,
    "lzo": false,
    "./src/lib/load-wasm/load-wasm-node.ts": "./src/lib/load-wasm/load-wasm-browser.ts"
  },
  "dependencies": {
    "@loaders.gl/compression": "3.2.0-alpha.1",
    "@loaders.gl/loader-utils": "3.2.0-alpha.1",
    "@loaders.gl/schema": "3.2.0-alpha.1",
    "async-mutex": "^0.2.2",
    "brotli": "^1.3.2",
    "bson": "^1.0.4",
    "int53": "^0.2.4",
    "lz4js": "^0.2.0",
    "lzo": "^0.4.11",
    "node-int64": "^0.4.0",
    "object-stream": "0.0.1",
    "parquet-wasm": "^0.3.1",
    "snappyjs": "^0.6.0",
    "thrift": "^0.14.2",
    "varint": "^5.0.0",
    "zstd-codec": "^0.1"
  },
  "peerDependencies": {
    "apache-arrow": "*"
  },
  "devDependencies": {
    "@types/bson": "^4.0.0",
    "@types/node": "^10.14.15",
    "@types/node-int64": "^0.4.29",
    "@types/thrift": "^0.10.8",
    "@types/varint": "^5.0.0",
    "apache-arrow": "^4.0.0"
  }
}
